  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/scripts/stub.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/scripts/preprocessor.js"
    },
    "testRegex": ".*/test/.*/.*\\.spec\\.(ts|tsx|js)$"
  }
